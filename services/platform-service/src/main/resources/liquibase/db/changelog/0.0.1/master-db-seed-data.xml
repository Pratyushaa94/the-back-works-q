<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.33.xsd">
    <!-- Seed data for lookup tables. -->
    <changeSet id="0.0.1-dml-1" author="Subbu">
        <loadData file="db/changelog/0.0.1/seed-data/tenant-resource-type.csv" tableName="tenant_resource_type">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/tenant-resource-status.csv" tableName="tenant_resource_status">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/data-type.csv" tableName="data_type">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/tenant-status.csv" tableName="tenant_status">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/tenant-category.csv" tableName="tenant_category">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/industry-vertical.csv" tableName="industry_vertical">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
        <loadData file="db/changelog/0.0.1/seed-data/tenant-type.csv" tableName="tenant_type">
            <column header="code" name="code" type="STRING"/>
            <column header="name" name="name" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
    </changeSet>
</databaseChangeLog>